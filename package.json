{
  "name": "nestjs-env",
  "version": "1.0.0",
  "description": "Using this library you can easily work with environment variables",
  "repository": {
    "type": "git",
    "url": "https://github.com/AntsiferovMaxim/nestjs-env"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.json",
    "prepublish": "yarn build",
    "test": "env-cmd -f ./src/__tests__/.env jest",
    "test:watch": "env-cmd -f ./src/__tests__/.env jest --watch",
    "test:coverage": "env-cmd -f ./src/__tests__/.env jest --coverage"
  },
  "keywords": [
    "nestjs",
    "config",
    "nestjs-env",
    "env"
  ],
  "author": "Antsiferov Maxim <antsiferovmaximv@gmail.com>",
  "license": "ISC",
  "peerDependencies": {
    "@nestjs/common": "^6.10.2"
  },
  "devDependencies": {
    "@nestjs/common": "^6.10.2",
    "@nestjs/core": "^6.10.10",
    "@nestjs/platform-express": "^6.10.10",
    "@nestjs/testing": "^6.10.10",
    "@types/jest": "^24.0.23",
    "env-cmd": "^10.0.1",
    "jest": "^24.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./__tests__/coverage",
    "testEnvironment": "node"
  }
}
